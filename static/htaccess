# ==================================
#  Start for analytics Config
# ==================================

RewriteRule ^/analytics.txt/? https://www.plastk.ca/analytics.txt [R=301,L]
RewriteRule ^/analytics.txt/? https://plastk.ca/analytics.txt [R=301,L]

# ==================================
#  End for analytics config
# ==================================

# ==================================
#  Start for apple deep linking config
# ==================================

RewriteRule ^/.well-known/apple-app-site-association/? https://www.plastk.ca/.well-known/apple-app-site-association [R=301,L]
RewriteRule ^/.well-known/apple-app-site-association/? https://plastk.ca/.well-known/apple-app-site-association [R=301,L]

# ==================================
#  End apple deep linking config
# ==================================

RewriteBase /
RewriteRule ^index\.html$ - [L]
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule . /index.html [L]
