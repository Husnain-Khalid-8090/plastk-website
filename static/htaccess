RewriteEngine On

# Forced Tier Linking Code
RewriteRule ^signup-([0-9]+)\.html$ https://www\.affiliates\.plastk\.ca/recruit\.php?ref=$1 [R=301,L]

# TextAds
RewriteRule ^([0-9]+)-([a-z]+)-([0-9]+)\.html$ https://www\.affiliates\.plastk\.ca/idevads\.php?id=$1&ad=$3 [R=301,L]
RewriteRule ^([0-9]+)-([a-z]+)-([0-9]+)-([0-9]+)\.html$ https://www\.affiliates\.plastk\.ca/idevads\.php?id=$1&ad=$3&page=$4 [R=301,L]

# Standard Links
RewriteRule ^([0-9]+)\.html$ https://www\.affiliates\.plastk\.ca/plastkaffiliatetraffic\.php?id=$1 [R=301,L]
RewriteRule ^([0-9]+)-([0-9]+)\.html$ https://www\.affiliates\.plastk\.ca/plastkaffiliatetraffic\.php?id=$1&page=$2 [R=301,L]
RewriteRule ^([0-9]+)-([0-9]+)-([0-9]+)\.html$ https://www\.affiliates\.plastk\.ca/plastkaffiliatetraffic\.php?id=$1&page=$2&set=$3 [R=301,L]
RewriteRule ^([0-9]+)-([0-9]+)-([0-9]+)-([0-9]+)\.html$ https://www\.affiliates\.plastk\.ca/plastkaffiliatetraffic\.php?id=$1&page=$2&set=$3&link=$4 [R=301,L]
RewriteRule ^([0-9]+)-([0-9]+)-([0-9]+)-([0-9]+)-([a-zA-Z]+)\.html$ https://www\.affiliates\.plastk\.ca/plastkaffiliatetraffic\.php?id=$1&page=$2&set=$3&link=$4&keyword=$5 [R=301,L]
RewriteRule ^([0-9]+)-([0-9]+)-([0-9]+)-([0-9]+)-([a-zA-Z]+)-([0-9]+)\.html$ https://www\.affiliates\.plastk\.ca/plastkaffiliatetraffic\.php?id=$1&page=$2&set=$3&link=$4&keyword=$5&custom=$6 [R=301,L]
RewriteRule ^([0-9]+)-([0-9]+)-([0-9]+)-([0-9]+)-([a-zA-Z]+)-([0-9]+)-([0-9]+)\.html$ https://www\.affiliates\.plastk\.ca/plastkaffiliatetraffic\.php?id=$1&page=$2&set=$3&link=$4&keyword=$5&custom=$6&url=$7 [R=301,L]

