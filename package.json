{
  "name": "plastk-website",
  "version": "1.0.0",
  "private": true,
  "description": "plastk-website",
  "author": "aurangzeb khan",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "build": "cross-env ACTIVE_ENV=\"development\" gatsby build",
    "build:staging": "cross-env ACTIVE_ENV=\"staging\" gatsby build",
    "build:uat": "cross-env ACTIVE_ENV=\"uat\" gatsby build",
    "build:dev": "cross-env ACTIVE_ENV=\"dev\" gatsby build",
    "build:live": "cross-env ACTIVE_ENV=\"production\" gatsby build",
    "develop": "cross-env ACTIVE_ENV=\"development\" gatsby develop -p 3003",
    "start": "cross-env ACTIVE_ENV=\"development\" gatsby develop -p 3003",
    "start:staging": "cross-env ACTIVE_ENV=\"staging\" gatsby develop -p 3003",
    "start:uat": "cross-env ACTIVE_ENV=\"uat\" gatsby develop -p 3003",
    "start:dev": "cross-env ACTIVE_ENV=\"dev\" gatsby develop -p 3003",
    "start:live": "cross-env ACTIVE_ENV=\"production\" gatsby develop -p 3003",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint:all": "yarn lint:css && yarn lint:js",
    "lint:css": "stylelint \"./src/**/*.js\"",
    "lint:js": "eslint --ext jsx,js src/",
    "lint:js:fix": "eslint --ext jsx,js --fix src/",
    "pre:commit": "lint-staged",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@fontsource/material-icons": "^4.5.4",
    "@fontsource/material-icons-outlined": "^4.5.4",
    "@fontsource/montserrat": "^4.5.14",
    "@fontsource/rubik": "^4.5.14",
    "@linaria/babel-preset": "^4.4.3",
    "@linaria/core": "^4.2.8",
    "@linaria/react": "^4.3.6",
    "axios": "^1.3.4",
    "babel-plugin-styled-components": "^2.0.7",
    "cross-env": "^7.0.3",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gatsby": "^5.7.0",
    "gatsby-plugin-client-side-redirect": "^1.1.0",
    "gatsby-plugin-eslint": "^4.0.3",
    "gatsby-plugin-google-gtag": "^5.7.0",
    "gatsby-plugin-image": "^3.7.0",
    "gatsby-plugin-linaria": "^4.0.1",
    "gatsby-plugin-manifest": "^5.7.0",
    "gatsby-plugin-offline": "^6.7.0",
    "gatsby-plugin-sharp": "^5.7.0",
    "gatsby-plugin-sitemap": "^6.7.0",
    "gatsby-plugin-styled-components": "^6.7.0",
    "gatsby-source-filesystem": "^5.7.0",
    "gatsby-transformer-sharp": "^5.7.0",
    "html-react-parser": "^3.0.9",
    "husky": "^8.0.3",
    "js-yaml": "^4.1.0",
    "linaria": "^4.1.15",
    "lint-staged": "^13.1.2",
    "prettier": "^2.8.4",
    "public-ip": "^6.0.1",
    "rc-util": "^5.28.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-intersection-observer": "^9.4.3",
    "react-media-hook": "^0.5.0",
    "react-select": "^5.7.0",
    "react-share-rc-18": "^4.4.1",
    "react-slick": "^0.29.0",
    "react-toastify": "^9.1.1",
    "slick-carousel": "^1.8.1",
    "styled-components": "^5.3.6",
    "stylelint": "^15.2.0",
    "stylelint-config-recommended": "^10.0.1"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "eslint": "^8.35.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint --fix"
    ]
  }
}
